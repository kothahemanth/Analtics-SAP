{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:ovp",
      "version": "1.17.5",
      "toolsId": "c1eb359e-348d-44f9-af95-f5644bddfaf4"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.ovp": {},
        "sap.ui.rta": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "mainModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [],
      "targets": {}
    }
  },
  "sap.ovp": {
    "globalFilterModel": "mainModel",
    "globalFilterEntitySet": "BooksAnalytics",
    "enableLiveFilter": true,
    "considerAnalyticalParameters": false,
    "globalFilterControlType": "MacroFilterBar",
    "chartSettings": {
      "showDataLabel": true
    },
    "containerLayout": "resizable",
    "cards": {
      "StockChart": {
        "model": "mainModel",
        "template": "sap.ovp.cards.v4.charts.analytical",
        "settings": {
          "entitySet": "BooksAnalytics",
          "title": "Books Stock Overview",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#TitleStock",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#TitleStock",
          "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#stock",
          "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
          "navigation": "display"
        }
      },
      "Stock": {
        "template": "sap.ovp.cards.v4.table",
        "model": "mainModel",
        "settings": {
          "entitySet": "BooksAnalytics",
          "title": "Stock Data",
          "sortBy": "ID",
          "sortOrder": "ascending",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Stock",
          "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#stock",
          "listType": "condensed",
          "listFlavor": "bar",
          "maxItems": 7
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  }
}

